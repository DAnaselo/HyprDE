font_size 12
# font_family      family="SF Mono"
enable_audio_bell no
background_opacity 0.8
confirm_os_window_close 0
hide_window_decorations yes
map pgup            scroll_page_up
map pgdown          scroll_page_down


# BEGIN_KITTY_FONTS
font_family      family='SF Mono' postscript_name=SFMono-Regular
bold_font        SF-Pro-Rounded-Bold
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS
